                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- BEGIN: TABLE MODAL HANDLER -->
                <div class="modal fade bd-example-modal-lg" id="table-action-modal" tabindex="-1" role="dialog"
                    aria-labelledby="myLargeModalLabel10" aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="myLargeModalLabel10"></h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <!-- START: INSERT FORM-->
                            <div class="modal-body">
                                <!-- type number + step any = float type-->
                                <form id="form-action-modal">
                                    <div class="form-row">
                                        <div class="col-md-3 mb-3">
                                            <label for="AdressValid">Địa chỉ</label>
                                            <input type="text" class="form-control" name="AdressValid" id="AdressValid"
                                                placeholder="Địa chỉ lấy mẫu" spellcheck="false" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-3 mb-3">
                                            <label for="LatitudeValid">Toạ độ Latitude</label>
                                            <input type="number" step="any" class="form-control" id="LatitudeValid"
                                                placeholder="Toạ độ Lat" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-3 mb-3">
                                            <label for="LongitudeValid">Toạ độ Longitude</label>
                                            <input type="number" step="any" class="form-control" id="LongitudeValid"
                                                placeholder="Toạ độ Long" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-3 mb-3">
                                            <label for="DatetimeValid">Ngày giờ lấy mẫu</label>
                                            <input type="datetime-local" class="form-control" id="DatetimeValid">
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="col-md-4 mb-3">
                                            <label for="WindDegreeValid">Độ gió</label>
                                            <input type="number" step="any" class="form-control" id="WindDegreeValid"
                                                placeholder="Độ gió" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <label for="HumidityValid">Độ ẩm</label>
                                            <input type="number" step="any" class="form-control" id="HumidityValid" placeholder="Độ ẩm"
                                                required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <label for="WindSpeedValid">Tốc độ gió</label>
                                            <input type="number" step="any" class="form-control" id="WindSpeedValid"
                                                placeholder="Tốc độ gió" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="col-md-4 mb-3">
                                            <label for="WindDustValid">Bụi mịn</label>
                                            <input type="number" step="any" class="form-control" id="WindDustValid"
                                                placeholder="Bụi mịn" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <label for="SulfurDioxideValid">SO2</label>
                                            <input type="number" step="any" class="form-control" id="SulfurDioxideValid"
                                                placeholder="Sulfur Dioxit" required>
                                            <div class="invalid-feedback">Không hợp lệ</div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <label for="NitoDioxideValid">NO2</label>
                                            <input type="number" step="any" class="form-control" id="NitoDioxideValid"
                                                placeholder="Nito Dioxit" required>
                                            <div class="invalid-feedback">
                                                Không hợp lệ.
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <input type="hidden" id="actionId"/>
                                        <input type="hidden" id="actionType"/>
                                        <input type="submit" id="save" class="btn btn-primary"/>
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Huỷ</button>
                                    </div>
                                </form>
                            </div>
                            <!-- END: INSERT FORM-->
                        </div>
                    </div>
                </div>
                <!-- END: TABLE MODAL HANDLER -->

